{
    "name": "xanpena/laravel-smvc",
    "description": "SMVC Pattern CRUD Generator Library",
    "keywords": [
        "smvc",
        "laravel",
        "schema",
        "generator",
        "crud"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Xan Pena",
            "email": "xanpena@outlook.es"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "illuminate/console": "^5.5.0|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.5.0|^6.0|^7.0|^8.0",
        "illuminate/auth": "^6.0|^7.0|^8.0",
        "illuminate/bus": "^6.0|^7.0|^8.0",
        "illuminate/routing": "^6.0|^7.0|^8.0",
        "illuminate/translation": "^6.0|^7.0|^8.0",
        "illuminate/validation": "^6.0|^7.0|^8.0",
        "symfony/console": "^3.4|^4.0|^5.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.5|^4.0",
        "phpunit/phpunit": "^4.8.36|^5.7.27|^6.5.14|^7.5|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Xanpena\\SMVC\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Xanpena\\SMVC\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xanpena\\SMVC\\SMVCServiceProvider"
            ],
            "aliases": {
                "SMVC": "Xanpena\\SMVC\\SMVCFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
